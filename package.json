{
  "name": "iui-vue",
  "version": "0.1.4",
  "private": false,
  "author": "zgrhq",
  "license": "MIT",
  "description": "a personal private component library for vue",
  "main": "dist/iui-vue.min.js",
  "keywords": ["iui-vue","vue"],
  "scripts": {
    "lib": "vue-cli-service build --target lib --name iui-vue --dest dist src/index.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "src",
    "dist"
  ],
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "vue": "^2.6.6",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
